    //   "@routes/*": ["src/routes/*"],
    //   "@validation/*": ["src/validation/*"],
    //   "@shared/*": ["src/middlewares/shared/*"]
    // }
  },
  "include": ["src/**/*", "src/**/*.ts", "src/**/*.*.ts", "src/_shared/config/**/*.ts"]
}
