    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "baseUrl": "./",
    // "paths": {
    //   "@config/*": ["src/config/*"],
    //   "@controllers/*": ["src/controllers/*"],
    //   "@middlewares/*": ["src/middlewares/*"],
